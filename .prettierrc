{
    "plugins": ["prettier-plugin-tailwindcss"],
    "tailwindStylesheet": "./src/styles.css",
    "tailwindFunctions": ["class", "className", "styleClass"],
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "htmlWhitespaceSensitivity": "ignore",
    "bracketSameLine": false,
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser": "angular"
            }
        }
    ]
}
